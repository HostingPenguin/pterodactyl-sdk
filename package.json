{
    "name": "@hostingpenguin/pterodactyl-sdk",
    "version": "0.0.1",
    "description": "A sdk for the Pterodactyl game panel software",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jestconfig.json",
        "test:verbose": "jest --config jestconfig.json --silent=false",
        "build": "tsc",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "yarn build",
        "prepublishOnly": "yarn test && yarn lint",
        "preversion": "yarn format"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HostingPenguin/pterodactyl-sdk.git"
    },
    "author": "bram.hoven@hostingpengu.in",
    "keywords": [
        "Pterodactyl",
        "sdk",
        "HostingPengu.in",
        "Hostingpenguin"
    ],
    "license": "MIT",
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "dotenv": "^8.2.0",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "ts-jest": "^26.5.3",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.2.3"
    },
    "files": [
        "lib/**/*"
    ]
}
